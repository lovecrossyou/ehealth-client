.tabs .active-switch .switch-container .focus-line {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  width: 160rpx;
  position: absolute;
  bottom: 0;
  border-bottom: 4rpx solid #f07;
  -webkit-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;
}
.tabs .active-switch .switch-container .focus-line.fix {
  width: 250rpx;
}
