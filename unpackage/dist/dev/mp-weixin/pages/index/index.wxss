  position: relative;
  min-width: 160rpx;
  width: 160rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  height: 100rpx;
  -webkit-transition: 0.3s;
  -o-transition: 0.3s;
  transition: 0.3s;
